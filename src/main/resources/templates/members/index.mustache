<!--게시글 목록 페이지-->
{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>
    </tr>
    </thead>
    <tbody>
    <!-- model에 담긴 객체를 사용하기 위함   -->
    {{#memberList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/members/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/memberList}}
    </tbody>
</table>
<a href="/signup">New Member</a>


{{>layouts/footer}}